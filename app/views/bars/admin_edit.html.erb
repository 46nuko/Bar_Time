<h1>編集画面</h1>

<%= form_with model: @bar, url: admin_update_bars_path do |form| %>

  <h4>bar名</h4>
  <%= form.text_field :name %>

  <h4>住所</h4>
  <%= form.text_field :address %>

  <h4>タグ名</h4>
  <%= form.collection_select :tag_id, Tag.all, :id, :name, include_blank: "選択して下さい" %>

  <%= form.submit '保存' %>

<% end %>